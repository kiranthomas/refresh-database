{
  "name": "michaeljennings/refresh-database",
  "description": "A package to speed up your laravel database tests by only running the migrations once",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Jennings",
      "email": "michael.jennings91@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "orchestra/testbench": "^3.0",
    "symfony/yaml": "^3.3|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">5.0",
    "php-coveralls/php-coveralls": "^2.1",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "MichaelJennings\\RefreshDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelJennings\\RefreshDatabase\\Tests\\": "tests/"
    }
  }
}
